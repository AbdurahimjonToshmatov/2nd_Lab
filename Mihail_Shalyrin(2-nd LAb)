               //задание 12

            double kvadrat, krug, treyg = 0;
            double gradusi = Math.Sin(1) / 3.14 * 180;
            Console.Write("Введите значение r: ");
            double r = double.Parse(Console.ReadLine());
            if (r > 0)
            {
                Console.Write("Введите: Площадь квадрата или Площадь круга или Площадь треугольника ");
                string ploshad = Console.ReadLine();
                switch (ploshad)
                {
                    case "Площадь квадрата":
                        Console.WriteLine(kvadrat = Math.Pow(r, 2));
                        break;
                    case "Площадь круга":
                        Console.WriteLine(krug = 3.14 * Math.Pow(r, 2));
                        break;
                    case "Площадь треугольника":
                        Console.WriteLine(treyg = gradusi * Math.Pow(r, 2) * 0.5);
                        break;
                    default:
                        Console.WriteLine("неизвестное имя");
                        break;
            // задание 13
                  
            // задание 13
            Console.WriteLine("Введите значения A и B, где A меньшая сторона! ");
            Console.Write("Сторона A: ");
            float A = float.Parse(Console.ReadLine());
            Console.Write("Сторона B: ");
            float B = float.Parse(Console.ReadLine());
            
            
            if (A > 0 && B > 0 && A < B && (A < B + B) && (B < A + B))
            {
                Console.WriteLine("Введите что-то из набора слов(Кольцо,Прямоугольник,Треугольник) ");
                string ploshad = Console.ReadLine();
                switch (ploshad)
                {
                    case "Кольцо":
                       
                        Console.Write(3.14 * (Math.Pow(B, 2) + Math.Pow(A,2)));
                        break;
                    case "Прямоугольник":
                        Console.Write(A * B);
                        break;
                    case "Треугольник":
                        
                        Console.Write(A / 4 * (Math.Sqrt(4 * Math.Pow(B, 2) - Math.Pow(A, 2))));
                        break;

                }
            }
            else
            {
                Console.WriteLine("Числа введены неправильно!");
            // задание 4
               Console.Write("Введите количество точек: ");
            int length = int.Parse(Console.ReadLine());
            int counter = 0;
            Console.Write($"Введите радиус: ");
            int R = int.Parse(Console.ReadLine());
            Console.WriteLine("Вводите точки через Enter по примеру:\nТочка #1: 3.25 5.74 (x и y через пробел)");
            for (int i = 0; i < length; i++)
            {
                Console.Write($"Точка #{i + 1}: ");
                string[] points = Console.ReadLine().Split(' ');
                double x = double.Parse(points[0]);
                double y = double.Parse(points[1]);
                if (Math.Pow(x, 2) + Math.Pow(y, 2) < R) counter++; 
                
            }
            Console.WriteLine("Точек в кругу: " + counter);
            Console.ReadLine();
