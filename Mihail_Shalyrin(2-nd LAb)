            // задание 1

            Console.WriteLine("Введите кол-во учащихся");
            int kol = int.Parse(Console.ReadLine());
            double sum = 0;
            for (int i = 1; i <= kol; i++)
            {
                Console.Write($"Введите рост человека: {i} ");
                double rost = int.Parse(Console.ReadLine());
                if (rost <= 0)
                {
                    Console.WriteLine("Введено неправильно");
                    break;
                }
                else
                {
                    sum += rost;
                }
               
            }
            Console.WriteLine(sum / kol);
            Console.ReadLine();
            
            
            //  задание 9
           
            
            Console.WriteLine("количество участников");
            int s = int.Parse(Console.ReadLine());
            if (s > 0)
            {
                int[] koli = new int[s];
                for (int i = 0; i < koli.Length; i++)
                {
                    Console.Write($"участник с индексом №{i + 1}  имеет результат: ");
                    koli[i] = int.Parse(Console.ReadLine());
                }
                Array.Sort(koli);
                Console.WriteLine($"Лучший результат: {koli[0]} ");
                Console.ReadLine();
            }
            else
            {
                Console.WriteLine("Good try(");
            }
            
            //задание 11
              // задание 11
            Console.WriteLine("Hello World!");
            Console.Write("Кол-во учеников: ");
            int kol_vo = Convert.ToInt16(Console.ReadLine());
            int col = 0;
            float summa = 0;
            float ball = 0;
            for (int i = 0; i < kol_vo; i++)
            {
                Console.Write("введите оценку по математике: ");
                int Math = Convert.ToInt32(Console.ReadLine());
                Console.Write("Введите оценку по химии :");
                int chemistry = Convert.ToInt32(Console.ReadLine());
                Console.Write("Введите оценку по истории :");
                int hist = Convert.ToInt32(Console.ReadLine());
                Console.Write("Введите оценку по биологии:");
                int biol = Convert.ToInt32(Console.ReadLine());

                if ((Math <= 5) && (chemistry <= 5) && (hist <= 5) && (biol <= 5))
                {
                    if ((Math != 2) && (chemistry != 2) && (hist != 2) && (biol != 2))
                    {
                        col++;
                        ball = (Math + chemistry + hist + biol) / 4;
                        summa +=  ball;
                    }

            }
            Console.WriteLine($"Число сдавших экззамен: {col} Число неуспевающих студентовL {kol_vo - col} Средний балл группы без неуспевающих: {summa / col}");
            Console.ReadLine();
