            //task 8
            double x = double.Parse(Console.ReadLine());
            double y = 0;
            if (Math.Abs(x) >= 1)
            {
                y = 0;
            }

            else
            {
                y = Math.Pow(x, 2) - 1;
            }
            Console.WriteLine(y);
