//часть1 задание 5
            const double pi = 3.14;
            double Ploshad_kruga = 70;
            double Ploshad_kvad = 36.74;
            Console.WriteLine("круг поместиться в квадрат если диоганаль меньше сторонв квадрата");
            double dig_kr = Math.Sqrt(Ploshad_kruga / pi);
            double diog_kv = Math.Sqrt(Ploshad_kvad * 2);
            if (diog_kv <= dig_kr)
            {
                Console.WriteLine("Квадпат вписывается");
            }
            else
            {
                Console.WriteLine("Не умещается(");
            }
            double Ploshad_kruga2 = 0.86;
            double Ploshad_kvad2 = 0.74;
            Console.WriteLine("круг поместиться в квадрат если диоганаль меньше сторонв квадрата");
            double diog_kr2 = Math.Sqrt(Ploshad_kruga2 / pi);
            double diog_kv2 = Math.Sqrt(Ploshad_kvad2 * 2);
            if (diog_kv <= diog_kr2)
            {
                Console.WriteLine("Квадпат вписывается");
            }
            else
            {
                Console.WriteLine("Не умещается(");

            }
